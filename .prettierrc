{
    "printWidth": 80,
    "tabWidth": 2,
    "trailingComma": "none",
    "singleQuote": true,
    "parser": "babel",
    "semi": true,
    "htmlWhitespaceSensitivity": "css",
    "quoteProps": "as-needed",
  "overrides": [
    {
      "files": "*.css",
      "options": {
        "parser": "css"
      }
    },
    {
      "files": "*.scss",
      "options": {
        "parser": "scss"
      }
    }
  ]
}
